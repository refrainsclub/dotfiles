{
  "languages": {
    "Python": {
      "show_inline_completions": false
    }
  },
  "vim_mode": true,
  "base_keymap": "VSCode",
  "theme": "Catppuccin Latte",
  "buffer_font_size": 16,
  "relative_line_numbers": true,
  "buffer_font_family": "Hack Nerd Font",
  "ui_font_family": "Hack Nerd Font",
  "tab_bar": {
    "show": false
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "chat_panel": {
    "button": false
  },
  "collaboration_panel": {
    "button": false
  },
  "assistant": {
    "version": "2",
    "button": false
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  }
}
